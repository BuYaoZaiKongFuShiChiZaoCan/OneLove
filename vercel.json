{
  "version": 2,
  "name": "onelove-app",
  "functions": {
    "OneLove-backend/server.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "OneLove-backend/server.js"
    },
    {
      "src": "/Pages/(.*)",
      "dest": "/Pages/$1"
    },
    {
      "src": "/styles/(.*)",
      "dest": "/styles/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/font/(.*)",
      "dest": "/font/$1"
    },
    {
      "src": "/webfonts/(.*)",
      "dest": "/webfonts/$1"
    },
    {
      "src": "/music/(.*)",
      "dest": "/music/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 